{
  "name": "saas_live_playground_site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pnpm run --filter with-vite dev",
    "dev": "pnpm run start",
    "dev:qiankun": "pnpm run --filter @qiankun/app-* dev",
    "build": "pnpm run --filter @ezuikit/player-react build",
    "build:examples": "pnpm run --filter with-* build",
    "lint": "eslint --fix \"packages/*/src/*.{ts,tsx,js,jsx,mjs}\" \"common/*/src/*.{ts,tsx,js,jsx,mjs}\" \"playground/src/*.{ts,tsx,js,jsx,mjs}\"",
    "fmt": "prettier --write \"./**/*.{ts,tsx,js,jsx}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "eslint-config-xx": "^1.10.1",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint": "8.57.0",
    "only-allow": "^1.2.1",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.24.4",
    "tslib": "^2.8.1",
    "typescript": "5.3.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.3.1",
    "husky": "^9.0.11",
    "pretty-quick": "^4.0.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8",
    "react": ">=16.8"
  },
  "resolutions": {
    "es5-ext": "0.10.53",
    "@types/react": "17.0.14"
  }
}
